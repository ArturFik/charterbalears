{
  "version": 2,
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.\\w+)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "charterbalears.com"
        }
      ],
      "destination": "https://www.charterbalears.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
